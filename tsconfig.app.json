{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
  ],
  "exclude": ["test/**/__tests__/**/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"],
      "plotly.js-cartesian-dist-min": [
        "./node_modules/@types/plotly.js"
      ]
    }
  }
}
